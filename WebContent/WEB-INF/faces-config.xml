<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
    <lifecycle>
    	<phase-listener>br.com.curso.jsf.ouvinte.LifeCycleListener</phase-listener>
    </lifecycle>
    <converter>
    	<converter-id>meuConversor</converter-id>
    	<converter-class>br.com.curso.jsf.conversor.MeuConversor</converter-class>
    </converter>
    <validator>
    	<validator-id>meuValidador</validator-id>
    	<validator-class>br.com.curso.jsf.validador.MeuValidador</validator-class>
    </validator>
</faces-config>
